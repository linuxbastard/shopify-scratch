    jQuery.ajax({
 	type: 'POST',
     url: '/cart/add.js',
     data: 'quantity='+quantity+'&id='+variantId,
     dataType: 'json',
     success: function(response){
         jQuery.ajax({
             type: 'GET',
             url: '/cart.js',
             dataType: 'json',
             success: function(cartdata){
               console.log(cartdata);
                 jQuery('.cart-icon-wrap .cart-wrap').html('<span>'+cartdata.item_count+'</span>');
				 jQuery('.cart-notification .item-name').html(title);
				 jQuery('.cart-notification').show();
				 setTimeout(function(){ jQuery('.cart-notification').fadeOut( "slow" ); },1000);
				 jQuery('.widget_shopping_cart_content').html('');
				 var build_html = '<ul class="cart_list product_list_widget ">';
               	 var moneyformat = {{ shop.money_format | json }};
                 for(var i=0;i<cartdata.items.length;i++){
					var item = cartdata.items[i];
					console.log(item);
                   
                   var item_price = Shopify.formatMoney(item.price, moneyformat);
					build_html = build_html + '<li class="mini_cart_item"><a href="'+item.url+'" class="remove no-ajaxy" aria-label="Remove this item" data-product_id="'+item.id+'">×</a><a href="'+item.url+'" class="no-ajaxy"><img src="'+item.image+'" class="attachment-shop_thumbnail size-shop_thumbnail wp-post-image" alt="" >'+item.title+'</a><span class="quantity">'+item.quantity+' × <span class="woocommerce-Price-amount amount">'+item_price+'</span></span></li>';
				}
               build_html = build_html + '</ul>';
               build_html = build_html + '<p class="total"><strong>Subtotal:</strong> <span class="woocommerce-Price-amount amount">'+Shopify.formatMoney(cartdata.total_price, moneyformat)+'</span></p>';
               build_html = build_html + '<p class="buttons"><a href="/cart" class="button wc-forward no-ajaxy">View cart</a><a href="/checkout" class="button checkout wc-forward no-ajaxy">Checkout</a></p>';
				jQuery('.widget_shopping_cart_content').html(build_html);
               }
           });
       jQuery(this).text('View cart');
       jQuery(this).attr('href','/cart');
       }
   });
});
  jQuery('.mini_cart_item .remove').click(function(e){
